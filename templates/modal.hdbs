<div class="modal hide fade assign-on-solve-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-body">
    <h3><strong>{{modal_message}}</strong></h3>
  </div>
  <div class="modal-footer">
    <button class="btn reject" data-dismiss="modal" aria-hidden="true">{{t "modal.no"}}</button>
    <button class="btn btn-primary accept">{{t "modal.yes"}}</button>
  </div>
</div>
